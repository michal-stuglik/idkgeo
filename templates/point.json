{
  "type": "FeatureCollection",
  "key": 1,
  "crs": {
    "type": "name",
    "properties": {
      "name": "urn:ogc:def:crs:OGC:1.3:CRS84"
    }
  },
  "features": [
      "{% for point in points %}",
        {
          "type": "Feature",
          "properties": {
            "title": "{{ point.name }}",
            "description": "{{ point.description }}",
            "popupContent": "{{ point.pop_content }}"
          },
          "geometry": {
            "type": "Point",
            "coordinates": [
              {{ point.geo.x }} {{ point.geo.y}}
            ]
          }
        },
    "{% endfor %}"
  ]
}